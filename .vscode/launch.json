    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) GATEWAY - Attach to beerocks_controller",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/build/install/bin/beerocks_controller",
            "processId": "${command:pickRemoteProcess}",
            "pipeTransport": {
                "pipeCwd": "${workspaceRoot}",
                "pipeProgram": "docker",
                "pipeArgs": [
                    "exec",
                    "-i",
                    "gateway-${env:USER}",
                    "sh",
                    "-c"
                ],
                "debuggerPath": "/usr/bin/gdb"
            },
            "presentation": {
                "hidden": false,
                "group": "Gateway",
                "order": 1
            }
