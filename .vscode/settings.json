    "files.exclude": {
        "build": true
    },
    "editor.formatOnSave": true,
    "C_Cpp.default.defines": [
        "BEEROCKS_LINUX",
        "BEEROCKS_CONF_PATH=\"build/install/config\"",
        "BEEROCKS_BIN_PATH=\"build/install/bin\"",
        "BEEROCKS_VERSION=\"\"",
