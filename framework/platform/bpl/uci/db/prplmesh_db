config prplmesh 'config'
    option management_mode 'Multi-AP-Controller-and-Agent'
    option operating_mode 'Gateway'
    option enable '1'
    option onboarding '0'
    option master '1'
    option gateway '1'
    option rdkb_extensions '0'
    option band_steering '0'
    option client_roaming '0'
    option dfs_reentry '0'
    option passive_mode '1'
    option wired_backhaul '0'
    option operational '0'
    option ssid 'test_ssid'
    option mode_enabled 'WPA2-Personal'
    option wep_key '123456789a'
    option key_passphrase 'test_passphrase'
    option mem_only_psk '0'
    option backhaul_band 'auto'
    option certification_mode '1'
    option stop_on_failure_attempts '1'
    option no_vendor_specific '0'

config wifi-device 'radio0'
    option hostap_iface 'wlan0'
    option dcs_enable '0'
    option dcs_channel_pool '36-48,149-165'
    option dcs_interval_sec '15'
    option dcs_dwell_msec '40'
    option dcs_refresh_sec '86400'

